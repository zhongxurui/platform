.scroll {
  position: relative;
}

.scroll .allItem {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  flex-wrap: wrap;
  position: absolute;
  align-content: space-around;
  top: 10rpx;
}

.scroll .scroll_item {
  width: 180rpx;
  height: 260rpx;
  position: relative;
  /* background: peru; *//* border: 10rpx solid transpare; */
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 40rpx;
  text-align: center;
  overflow: hidden;
  /* border: 1px outset red; */
}

.scroll .scroll_item image {
  width: 180rpx;
  height: 180rpx;
}

.scroll .scroll_item .name {
  background: white;
  margin-top: -10rpx;
  font-weight: 400;
  font-size: 25rpx;
  line-height: 50rpx;
}

.scroll .scroll_item .value {
  /* background-color: rgba(98, 166, 243,0.5); */
  margin-top: -10rpx;
  font-weight: 400;
  font-size: 25rpx;
  line-height: 40rpx;
  color: #e45110;
}

.signBody {
