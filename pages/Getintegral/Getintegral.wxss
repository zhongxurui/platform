/* pages/Getintegral/Getintegral.wxss */
@import '../temp/temp.wxss';

page {
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: #5876f1;
  position: relative;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.Getindex {
  width: 100%;
  height: 100%;
  position: relative;
}

.shopTop {
  width: 100%;
  height: 60rpx;
  margin-top: 10rpx;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.shopTop .tips {
  width: 200rpx;
  height: 60rpx;
  position: relative;
  background-color: rgba(0, 0, 0, 0.2);
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 40rpx;
}

.shopTop .tips image {
  width: 42rpx;
  height: 42rpx;
  position: relative;
}

.shopTop .tips text {
  display: block;
  color: white;
  width: 120rpx;
  text-align: center;
}

.gold {
  display: block;
  width: 100rpx;
  height: 60rpx;
  color: white;
  line-height: 60rpx;
  position: relative;
  margin-left: 10rpx;
}

.gold .item {
  width: 100%;
  height: 30rpx;
  position: relative;
  text-align: center;
}

.gold view text {
  display: block;
  position: relative;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 20rpx;
}

.boby {
  width: 100%;
  height: 95%;
  position: relative;
  margin-top: 10rpx;
  background-color: white;
}

.playtitle {
  width: 100%;
  height: 100rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 10rpx;
}

.playtitle text {
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
  display: block;
  color: #469be4;
}

.playtitle image {
  width: 345rpx;
  height: 44rpx;
  display: block;
  position: relative;
}

.tuijiantitle {
  width: 100%;
  height: 60rpx;
  position: relative;
  /* display: flex;
    align-items: center;
    justify-content: center; */
  margin-top: -30rpx;
}

.tuijiantitle .tuijiantitle_text {
  width: 210rpx;
  display: block;
  color: #469be4;
  font-size: 25rpx;
  float: left;
  margin-top: 10rpx;
}

.tuijiantitle view {
  width: 100rpx;
  position: relative;
  height: 45rpx;
  margin-left: 20rpx;
  border-radius: 40rpx;
  background-color: #ee4d63;
  float: left;
  margin-top: 5rpx;
}

.tuijiantitle view text {
  line-height: 45rpx;
  text-align: center;
  color: white;
  font-size: 25rpx;
  display: block;
}

.swiper {
  width: 100%;
  height: 360rpx;
  position: relative;
  margin-top: 10rpx;
}

.swimge {
  width: 720rpx;
  height: 360rpx;
  position: relative;
}

.swimge .slide-image {
  width: 720rpx;
  height: 360rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.swimge .slide-image .slide_i_new {
  height: 300rpx;
  width: 700rpx;
  position: relative;
}

.swimge .slide-image .slide_i_new image {
  display: block;
  position: relative;
  width: 670rpx;
  height: 300rpx;
}

.btn_active {
  width: 100%;
  height: 200rpx;
  position: relative;
  margin-top: 15rpx;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  align-items: center;
  justify-content: center;
}

.active_item {
  width: 50%;
  height: 100%;
  position: relative;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  border-right: 1px solid rgba(0, 0, 0, 0.1);
}

.active_item image {
  width: 302rpx;
  height: 128rpx;
  position: relative;
}

