  margin-top: 20rpx;
}

.signBody .sign .signInfo .signItem_7 image {
  width: 259rpx;
  height: 133rpx;
  position: relative;
  margin: 0 auto;
  display: block;
}

.signBody .sign .signInfo .signItem_7 .goldnum {
  width: 280rpx;
  height: 40rpx;
  position: absolute;
  bottom: 4rpx;
  left: 50%;
  border-radius: 8rpx;
  margin-left: -140rpx;
  background: #ededed;
  display: flex;
  align-items: center;
  justify-content: center;
}

.signBody .sign .signInfo .signItem_7 .goldnum text {
  display: block;
  color: #e85f72;
  font-size: 25rpx;
}

.signBody .sign .signbtn {
  width: 400rpx;
  height: 80rpx;
  position: relative;
  margin: 0 auto;
  margin-top: 30rpx;
  border-radius: 20rpx;
  background-color: #29b0ef;
  color: white;
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  display: block;
}

